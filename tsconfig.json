{
  "compilerOptions": {
    "target": "ES2020",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "dist",
    "rootDir": "src",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "noEmitOnError": true,
    // "allowImportingTsExtensions":true,
    // "noEmit":true

  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
